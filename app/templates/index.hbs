<div class="row">
  <div class="col s3">
    {{login-form newLogIn="newLogIn"}}
    <div class="divider"></div>
    {{signup-form newSignUp="newSignUp"}}
  </div>

  <div class="col s9">
    <div class="row">
      <div class="col s7">
        <h4>View a Legislator</h4>
        <ul class="collection">
          {{#each model.allLegislators as |lawmaker|}}
            <li class="collection-item">{{lawmaker.first_name}} {{lawmaker.last_name}}</li>
          {{/each}}
        </ul>
      </div>
      <div class="col s5">
        {{current-user-tile logMeOut="logMeOut"}}
      </div>
    </div>
  </div>
</div>
